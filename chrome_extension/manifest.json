{
    "name": "Audio Manager Helper",
    "version": "0.1",
    "permissions": [
        "tabs", 
        "storage",
        "http://localhost/*"
    ],
    "description": "Manage Audio Automatically.",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}